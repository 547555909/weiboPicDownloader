{
  "name": "weibo-pic-downloader",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "electron:generate-icons": "electron-icon-builder --input=./public/icon.png --output=build --flatten",
    "electron:build": "vue-cli-service electron:build",
    "electron:build-win": "vue-cli-service electron:serve -- --win nsis",
    "electron:build-linux": "vue-cli-service electron:serve -- --linux deb",
    "electron:build-mac": "vue-cli-service electron:build --macos",
    "electron:build-all": "vue-cli-service electron:build -- --linux deb --win nsis --macos ",
    "electron:serve": "vue-cli-service electron:serve",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps"
  },
  "main": "background.js",
  "dependencies": {
    "element-ui": "^2.14.0",
    "request": "^2.88.2",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "electron": "^9.0.0",
    "electron-devtools-installer": "^3.1.0",
    "electron-icon-builder": "^1.0.2",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "vue-cli-plugin-electron-builder": "~2.0.0-rc.5",
    "vue-template-compiler": "^2.6.11"
  },
  "description": "A no-login tool to download picture from Weibo",
  "author": {
    "name": "jasonkay",
    "email": "271226192@qq.com",
    "url": "https://jasonkayzk.github.io/"
  }
}
